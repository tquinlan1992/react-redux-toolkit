{
  "name": "react-redux-toolkit",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "private": true,
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:tsc": "tsc --noEmit",
    "pretty": "prettier --config ../../.prettierrc.json --check './src/**/*.{js,ts}'",
    "test:ci": "jest",
    "test:watch": "jest --watchAll --runInBand",
    "test:inspect": "node --inspect $(npm bin)/jest --watch --no-cache --runInBand"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-scoped"
    }
  },
  "eslintIgnore": [
    "/**/*.js"
  ],
  "dependencies": {
    "@hookform/resolvers": "^2.8.5",
    "@reduxjs/toolkit": "^1.7.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.176",
    "@types/node": "^15.14.1",
    "@types/react-redux": "^7.1.20",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module-scoped": "^1.0.3",
    "jest": "^27.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "^0.0.6",
    "react-hook-form": "^7.22.1",
    "react-redux": "^7.2.6",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.4.4",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.7.0",
    "lodash": "^4.17.21"
  }
}
