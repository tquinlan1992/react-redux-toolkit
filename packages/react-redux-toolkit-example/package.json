{
  "name": "@tquinlan1992/react-redux-toolkit-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^1.2.21",
    "@craco/craco": "^6.4.3",
    "@reduxjs/toolkit": "^1.7.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@tquinlan1992/react-redux-toolkit": "^0.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.10.2",
    "aws-amplify": "^4.3.3",
    "components": "*",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^4.0.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.4",
    "web-vitals": "^1.0.1",
    "webpack": "~4.44.2",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev": "PORT=8000 craco start",
    "build": "craco build",
    "type-check": "tsc --noEmit",
    "test:watch": "craco test --runInBand",
    "test:ci": "JEST_JUNIT_OUTPUT_FILE=../../reports/junit/${PWD##*/}-junit.xml craco test --watchAll=false",
    "eject": "craco eject",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "serve": "serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:@typescript-eslint/recommended",
      "prettier/react"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "import/no-anonymous-default-export": 0,
          "typescript-eslint/explicit-module-boundary-types": 2
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
